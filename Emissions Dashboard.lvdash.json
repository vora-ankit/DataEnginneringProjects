{
  "datasets": [
    {
      "name": "eb13b281",
      "displayName": "Location Data",
      "queryLines": [
        "select latitude,longitude,`GHG emissions mtons CO2e` as Emissions from emissions.default.emissions_data"
      ]
    },
    {
      "name": "067004f9",
      "displayName": "Emissions per person",
      "queryLines": [
        "SELECT \r\n",
        "  county_state_name,\r\n",
        "  population,\r\n",
        "  CAST(\r\n",
        "    REGEXP_REPLACE(`GHG emissions mtons CO2e`, '[^0-9.]', '') AS DOUBLE\r\n",
        "  ) / CAST(\r\n",
        "    REGEXP_REPLACE(population, '[^0-9.]', '') AS DOUBLE\r\n",
        "  ) AS emissions_per_person\r\n",
        "FROM \r\n",
        "  emissions.default.emissions_data AS ed\r\n",
        "  ORDER BY emissions_per_person DESC\r\n"
      ],
      "catalog": "emissions",
      "schema": "default"
    },
    {
      "name": "efe91a13",
      "displayName": "Total Emissions Per State",
      "queryLines": [
        "SELECT \r\n",
        "  state_abbr,\r\n",
        "  SUM(\r\n",
        "    CAST(\r\n",
        "      REGEXP_REPLACE(`GHG emissions mtons CO2e`, '[^0-9.]', '') AS DOUBLE\r\n",
        "    ) \r\n",
        "  ) AS total_emissions\r\n",
        "FROM \r\n",
        "  emissions.default.emissions_data AS ed\r\n",
        "GROUP BY \r\n",
        "  state_abbr\r\n",
        "ORDER BY \r\n",
        "  total_emissions DESC\r\n",
        "LIMIT 10"
      ],
      "catalog": "emissions",
      "schema": "default"
    },
    {
      "name": "88c334c2",
      "displayName": "County Shaming",
      "queryLines": [
        "SELECT \r\n",
        "  county_state_name,\r\n",
        "  population,\r\n",
        "  CAST(\r\n",
        "    REGEXP_REPLACE(`GHG emissions mtons CO2e`, '[^0-9.]', '') AS DOUBLE\r\n",
        "  )  AS Total_Emissions\r\n",
        "FROM \r\n",
        "  emissions.default.emissions_data AS ed\r\n",
        "  ORDER BY Total_Emissions DESC\r\n",
        "LIMIT \r\n",
        "  10"
      ],
      "catalog": "emissions",
      "schema": "default"
    }
  ],
